#include <stdio.h>

int main() {
    int start = 1;
    int end = 7;
    int step = 2;
    int rows = 3;
    int i, j;
    
    for (i = 1; i <= rows; i++) {
        for (j = start; j < end; j += step) {
            printf("%d\t", j);
        }
        printf("\n");
        start += 4;
        end += 4;
    }
    
    return 0;
}
