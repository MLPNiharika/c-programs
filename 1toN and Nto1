#include <stdio.h>

int main() {
    int n, i;

    printf("Enter a number: ");
    scanf("%d", &n);

    // Printing numbers from 1 to n
    printf("Numbers from 1 to %d\n", n);
    for (i = 1; i <= n; i++) {
        printf("%d ", i);
    }
    printf("\n");

    // Printing numbers from n to 1
    printf("Numbers from %d to 1\n", n);
    for (i = n; i >= 1; i--) {
        printf("%d ", i);
    }
    printf("\n");

    return 0;
}
